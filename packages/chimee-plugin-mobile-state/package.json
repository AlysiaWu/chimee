{
  "name": "chimee-plugin-mobile-state",
  "version": "0.0.21",
  "description": "state for chimee",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "jsnext:main": "lib/index.mjs",
  "browser": "lib/index.browser.js",
  "scripts": {
    "build": "npm run tsc && npm run types && npm run mv-assets && npm run b-common && npm run b-es && npm run b-umd && npm run b-min",
    "b-common": "rollup -c build/rollup.config.common.js",
    "b-es": "rollup -c build/rollup.config.es.js",
    "b-umd": "rollup -c build/rollup.config.umd.js",
    "b-umd-w": "rollup -c build/rollup.config.umd.js -w",
    "b-min": "rollup -c build/rollup.config.min.js",
    "dev": "npm run tscw & npm run b-umd-w",
    "tsc": "tsc --outDir ./lib/esnext",
    "tscw": "tsc --outDir ./lib/esnext -w --preserveWatchOutput",
    "types": "tsc --outDir ./lib/types --emitDeclarationOnly",
    "typesw": "tsc --outDir ./lib/types --emitDeclarationOnly -w",
    "lint": "tslint -c ./tslint.json 'src/**/*.ts' --fix && eslint . --fix",
    "version": "npm run build && git add -A lib",
    "postversion": "git push --no-verify && git push --tags --no-verify",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Chimeejs/chimee-plugin-state.git"
  },
  "keywords": [
    "plugin",
    "chimee",
    "state",
    "video",
    "mobile"
  ],
  "author": "yandeqiang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Chimeejs/chimee-plugin-state/issues"
  },
  "homepage": "https://github.com/Chimeejs/chimee-plugin-state#readme",
  "dependencies": {
    "@babel/runtime-corejs2": "^7.0.0",
    "@types/dom-helpers": "^3.4.1",
    "chimee-plugin-gesture": "^2.0.0-alpha.3",
    "dom-helpers": "^3.4.0",
    "toxic-decorators": "^0.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "chimee": "^2.0.0-alpha.3",
    "cpx": "^1.5.0",
    "cssnano": "^3.10.0",
    "eslint": "^4.2.0",
    "eslint-config-egg": "^7.4.1",
    "eslint-plugin-jest": "^22.7.2",
    "postcss-base64": "^0.7.1",
    "rollup": "^0.51.8",
    "rollup-plugin-babel": "^4.0.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^0.5.3",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-string": "^2.0.2",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-plugin-visualizer": "^2.4.4",
    "stylelint": "^8.3.0",
    "stylelint-config-standard": "^18.0.0",
    "toxic-utils": "^0.4.3",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2"
  }
}
